<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tws.mapper.ParkingBoyMapper">
	<select id="selectAllParkingBoys"
		resultType="tws.entity.ParkingBoy">
		select * from parkingboy;
	</select>
	<insert id="addOneParkingBoy" parameterType="java.lang.String">
		insert into
		parkingboy values(#{id})
	</insert>
</mapper>