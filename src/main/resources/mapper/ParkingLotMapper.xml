<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tws.mapper.ParkingLotMapper">
	<select id="selectAllParkingLots"
		resultType="tws.entity.ParkingLot">
		select * from parkinglot;
	</select>
	<insert id="addOneParkingLot" parameterType="tws.entity.ParkingLot">
		insert into
		parkingboy values(#{parkinglot})
	</insert>
</mapper>